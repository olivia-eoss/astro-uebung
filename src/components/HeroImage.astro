---
import { Image } from "astro:assets";
import startseite from "../assets/startseite.jpg";
---

<div>
  <Image src={startseite} alt="This is my Logo." />
</div>

<style>
  div {
    width: 100vw;
    height: 50vh;
  }
  img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
  }
</style>
